{
  "manifest_version": 2,
  "name": "Plato Remote Display",
  "version": "0.1",
  "description": "Plato Remote Display",
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "<all_urls>",
    "storage",
    "sessions"
  ]
}
